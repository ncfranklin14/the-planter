<h2 class="center-align" >Login</h2>
<form class="center-align center-align" id="login">
    <input class="blue-text center-align  " type="text" placeholder="username" id="loginUsername">
    <input class="blue-text center-align" type="password" placeholder="password" id="loginPassword" type="password">
    <button class=" waves-effect teal darken-3 btn-small" type="submit">Login!</button>
</form>
<h2 class="center-align">Signup</h2>
<form class ="center-align" id="signup">
    <input class="black-text center-align" type="text" placeholder="username" id="signupUsername">
    <input class="black-text center-align" type="password" placeholder="password" id="signupPassword" type="password">
    <button class=" waves-effect teal darken-3 btn-small" type="submit">Sign up!</button>
</form>
<script src="/js/login.js"></script>

<script src="https://upload-widget.cloudinary.com/global/all.js" type="text/javascript"></script>
