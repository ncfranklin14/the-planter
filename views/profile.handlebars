<h1>Welcome back {{username}}</h1>
<h2>All your blogs:</h2>
{{#each blogs}}
<h3>{{title}}</h3>
<p>{{content}}</p>
<img src ={{imageUrl}}></img>
<button>delete</button>
<hr>
{{/each}}
<h3>Write about your plant:</h3>
<form action="" id="newBlog">
    <input class="blue-text text-darken-2 card-panel grey darken-3" type="text" id="title" placeholder="title">
    <textarea class="blue-text text-darken-2 card-panel grey darken-3" id="content" cols="30" rows="10"></textarea>
    <button>Submit</button>
</form>
<h3>Add Photo of your Plant</h3>
<button id="upload_widget" class="cloudinary-button waves-effect waves-light btn">Upload files</button>
<img id="uploadedimage" src="">
</img>
<script src="https://upload-widget.cloudinary.com/global/all.js" type="text/javascript">
</script>
<script src="https://widget.cloudinary.com/v2.0/global/all.js" type="text/javascript">
</script>

<!-- local upload instantiation -->
<script src="/js/profile.js" type="text/javascript"></script>

